x = 0
y = 0
w = 0
d = 0
def setup():
    global x,y,w,d
    size(600,600)
    #rectMode(CENTER)
    background(255,255,255)
    x = 200
    y = 200
    w = 250
    d = 130
def draw():
    global x,y,w,d
    rect(x,y,w,d)
def mouseClicked():
    if ((mouseX > x) and (mouseX < (x + w)) and (mouseY > y) and (mouseY < (y + d))):
        background(255, 0, 0)
    else:
        background(255,255,255)
