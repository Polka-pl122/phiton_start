x = 0
y = 0
x1 = 0
y1 = 0
def setup():
     global x,y,x1,y1
     size(600,400)
     frameRate(1000)
     strokeWeight(10)
     
def draw():
    global x,y,x1,y1
    stroke(random(0,255),random(0,255),random(0,255))
    line(x,y,x1,y1)
    x = 300
    y = 200
    x1 = random(0,600)
    y1 = random(0,400)
