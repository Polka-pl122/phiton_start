x = 0
y = 0
x1 = 0
y1 = 0
x2 = 0
y2 = 0
x3 = 0
y3 = 0
def setup():
    global x,y,x1,y1,x2,y2,x3,y3
    size(1000,600)
    background(0,43,126)
    stroke(255,255,255)
    strokeWeight(5)
    
def draw():
    global x,y,x1,y1,x2,y2,x3,y3
    x = random(0,400)
    y = random(0,250)
    x1 = random(600,1000)
    y1 = random (0,250)
    x2 = random(0,400)
    y2 = random(350,650)
    x3 = random(600,1000)
    y3 = random(350,650)
    point(x,y)
    point(x1,y1)
    point(x2,y2)
    point(x3,y3)
