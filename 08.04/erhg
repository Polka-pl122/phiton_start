z = 0
x = 0
y = 0

def setup():
    global z,x,y
    size(600,600)
    textSize(20)
    textAlign(CENTER,CENTER)
    x = 20
    y = 20

def draw():
    global z,x,y

def keyPressed():
    global z,x,y
    text(key,x,y)
    x = x + 30
    if (key == ENTER):
        y = y + 50
        x = 20

    if(key == TAB):
        x = x + 50
