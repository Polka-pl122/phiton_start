angle = 0
x = 0 
y = 0
w = 0
d = 0
def setup():
    global angle,x,y,w,d
    size(800,800)
    x = 100

def draw():
    global angle,x,y,w,d
    translate(400,400)
    rotate(radians(angle))
    fill(random(0,255),random(0,255),random(0,255))
    w = random(5,100)
    d = random(5,100)
    y = 0
    ellipse(x,y,w,d)
    angle = angle + 3
    x = x + 1
