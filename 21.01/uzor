angel = 0
angel1 = 0
x = 0
y = 0
def setup():
    global angel,x,y,angel1
    size(800,800)
    x = 100
    y = 100
def draw():
    global angel,x,y,angel1
    #background(160)
    push()
    translate(400,400)
    rotate(radians(angel))
    push()
    translate(200,200)
    rotate(radians(angel1))
    rect(x,y,50,50)
    angel = angel + 1
    angel1 = angel1 + 5
    pop()
    pop()
