x = 0
y = 0
w = 0
d = 0
def setup():
    global x,y,w,d
    size(500,500)
    frameRate(10)
def draw():
    global x,y,w,d
    background(255)
    for step in range(10):
        colorMode(HSB,360,100,100)
        fill(random(0,360),random(0,100),random(0,100))
        x = random(0,500)
        y = random(0,500)
        w = random(10,100)
        d = random(10,100)
        ellipse(x,y,w,d)
